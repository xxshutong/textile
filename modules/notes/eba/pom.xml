        </dependency>
        <dependency>
            <groupId>net.sourceforge.ezmorph</groupId>
            <artifactId>com.springsource.net.sf.ezmorph</artifactId>
        </dependency>
        <dependency>
            <groupId>org.apache.commons</groupId>
            <artifactId>com.springsource.org.apache.commons.beanutils</artifactId>
