<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5" metadata-complete="true">

    <display-name>Integration Tests Servlets - Framework Services</display-name>

    <servlet>
        <display-name>IntegrationTestsServlet</display-name>
        <servlet-name>IntegrationTestsServlet</servlet-name>
        <servlet-class>org.opentaps.testsuit.web.IntegrTestsServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>IntegrationTestsServlet</servlet-name>
        <url-pattern>/runTests/*</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
        <welcome-file>index.htm</welcome-file>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>

</web-app>